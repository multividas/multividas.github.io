{
  "name": "multividas.github.io",
  "private": true,
  "version": "1.0.0",
  "description": "Multividas.com is a social media and blogging platform where people can share their short texts and posts, leaving comments and have discussions on threads.",
  "keywords": [
    "multividas",
    "multividas api documentation",
    "multividas threads",
    "multividas blogs"
  ],
  "author": "soulaimane yahya <contact@soulaimaneyahya.com> (https://soulaimaneyahya.com/)",
