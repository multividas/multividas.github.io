    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "explore": "gridsome explore"
  },
  "dependencies": {
    "gridsome": "^0.7.0"
  },
  "engines": {
    "node": "^18"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-plugin-node": "^11.1.0",
